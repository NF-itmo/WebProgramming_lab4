<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>CheckerService</servlet-name>
        <servlet-class>org.web3.services.checker.CheckerService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckerService</servlet-name>
        <url-pattern>/checker</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HistoryService</servlet-name>
        <servlet-class>org.web3.services.history.HistoryService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HistoryService</servlet-name>
        <url-pattern>/history</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AuthService</servlet-name>
        <servlet-class>org.web3.services.auth.AuthService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AuthService</servlet-name>
        <url-pattern>/auth</url-pattern>
    </servlet-mapping>
</web-app>